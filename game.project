[project]
title = Snowfall Survivor
version = 0.1
dependencies = https://github.com/CodingKitsune/Riritools/archive/master.zip

[bootstrap]
main_collection = /main/main.collectionc
render = /riritools/render/render.renderc

[input]
game_binding = /input/game.input_bindingc
repeat_interval = 0.01
repeat_delay = 0.02

[display]
width = 1280
height = 720

[physics]
scale = 0.02
gravity_y = 0.0

[script]
shared_state = 1

